   "source": [
    "import pandas as pd\n",
    "julho = pd.read_csv('focos mensal.csv', sep=',')\n", # lendo os dados na Data base do INPE. Esses dados são de queimadas do mundo TODO
    "logic = julho[\"pais\"] == \"Brasil\"\n",  # filtrando os dados para deixar apenas as queimadas do Brasil
    "julho = julho.loc[logic]\n",
    "logic = julho[\"bioma\"] == \"Cerrado\"\n", # filtrando os dados para deixar apenas as queimadas do Cerrado
    "julho = julho.loc[logic]\n",
    "julho.pop('frp')\n", # Eliminando coluna desnecessária, com muitos Nans
    "julho.pop('id')\n", # Eliminando coluna desnecessária
    "julho.pop('geom')\n", # Eliminando coluna desnecessária
    "julho"
   ]
  },
